{
  
  "DeployDescriptor": [
    {
    "Property": "CreatedBy",
    "Assign": "replace-with-email"
    },
    {
    "Property": "Host",
    "Assign": "replace-with-instruqt-host"
    },
    {
    "Property": "Application",
    "Replace": [{ "zzz(.*)": "replace-with-usercode$1" }] 
    },
    {
    "Property": "SubApplication",
    "Replace": [{ "zzz(.*)": "replace-with-usercode$1" }] 
    },
    {
    "Property": "@",
    "ApplyOn": {"Type": "Folder"},
    "Replace": [{"zzz(.*)": "replace-with-usercode$1"}]
    },
    {
		"Property": "ReferencePath",
		"ApplyOn": {
		"Type": "SubFolder",
		"@": "payments_ref"
		},
		"Replace": [ { "zzz(.*)": "replace-with-usercode$1"}]
	}
  ]
}
