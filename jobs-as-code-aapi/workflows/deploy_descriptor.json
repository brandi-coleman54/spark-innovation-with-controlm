{
  "DeployDescriptor": [
    {
      "Property": "@",
      "ApplyOn": { "Type": "Folder" },
      "Replace": [
        { "^(.+?)_(.*)$": "$1-$2" }
      ]
    },
    {
        "Property": "@",
        "ApplyOn": { "Type": "Job" },
        "Replace": [
          { "^(.*)$": "replace-with-usercode-$1" }
        ]
    },
    {
      "Property": "Sequence",
      "ApplyOn": { "Type": "Flow" },
      "Replace": [
        { "^(.*)$": "replace-with-usercode-$1" }
      ]
    }
    ]
  }
